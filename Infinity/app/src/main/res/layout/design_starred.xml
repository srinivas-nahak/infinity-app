<?xml version="1.0" encoding="utf-8"?>
<custom_views_and_styles.NewFrameLay
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/album_frame_lay"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="2dp"
    android:padding="1dp"
    >

  <!--  <ImageView
        android:id="@+id/starred_img_holder"
        android:layout_width="match_parent"
        android:layout_height="120dp"
        android:scaleType="centerCrop"
        />-->
    <custom_views_and_styles.RoundedImageView
        android:id="@+id/starred_img_holder"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:adjustViewBounds="true"
        android:scaleType="centerCrop" />
    <View
        android:id="@+id/tintLay"
        android:layout_width="match_parent"
        android:layout_height="120dp"
        android:clickable="true"
        android:focusableInTouchMode="true"
        android:focusable="true"/>
   <ImageView
       android:id="@+id/multiPicIndicator"
       android:layout_width="20dp"
       android:layout_height="20dp"
       android:layout_marginRight="0.5dp"
       android:layout_marginEnd="1.5dp"
       android:visibility="gone"
       android:src="@drawable/triple_dot_shadow"
       android:layout_gravity="end|top"/>
</custom_views_and_styles.NewFrameLay>